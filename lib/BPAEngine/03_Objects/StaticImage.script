
# -------------------------------------------------------------------------
# Static Image
# -------------------------------------------------------------------------

StaticImage = fun (config)
  {
    new_object =  [] | global.StaticImage | global.Object(config);
    new_object.class = "StaticImage";
    new_object.z = config.z | 0;
    new_object.image = Image(config.image_path);
    new_object.sprite.SetImage(new_object.image);
    return new_object;
  } | [];
